    <p rend="left-middle" style="tall">
        This is the left middle column. Add “style=tall” if it needs more height.
    </p>

    <!-- Column: right, middle -->
    <p rend="right-middle">
        This is the right middle column.
    </p>

    <!-- Column: left, bottom -->
    <p rend="left-bottom">
